{
  "name": "tsup-template",
  "version": "1.0.0",
  "description": "",
  "keywords": [],
  "license": "ISC",
  "author": "",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "tsc --diagnostics",
    "build": "tsup --clean --splitting",
    "dev": "pnpm run build --watch",
    "prepare": "husky install",
    "release": "pnpm build && pnpm test && bumpp && pnpm publish",
    "test": "vitest run",
    "test:u": "vitest -u"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,md,html,css,json}": "prettier --write"
  },
  "devDependencies": {
    "bumpp": "^9.4.0",
    "husky": "^8.0.0",
    "lint-staged": "^13.0.3",
    "prettier": "^3.2.2",
    "prettier-plugin-organize-imports": "^3.2.4",
    "prettier-plugin-packagejson": "^2.4.9",
    "typescript": "^5.3.2",
    "vitest": "^0.33.0"
  },
  "packageManager": "pnpm@8.15.3+sha1.64838798f519c18029c1e8a1310e16101fc2eda0",
  "publishConfig": {
    "access": "public"
  }
}